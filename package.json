{
  "name": "react-seed",
  "version": "1.0.0",
  "description": "Initial directory structure and package.json as a seed for new React based projects.",
  "scripts": {
    "start": "cross-env NODE_ENV=development node devServer.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andrewcoelho/react-seed"
  },
  "bugs": {
    "url": "https://github.com/andrewcoelho/react-seed/issues"
  },
  "homepage": "https://github.com/andrewcoelho/react-seed#readme",
  "keywords": [
    "seed",
    "boilerplate",
    "react",
    "redux",
    "es6",
    "babel",
    "hot",
    "webpack"
  ],
  "main": "index.js",
  "author": "Andrew Coelho <info@andrewcoelho.com> (http://andrewcoelho.com)",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "cross-env": "^2.0.0",
    "eslint": "^3.1.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-react": "^5.2.2",
    "react-hot-loader": "^3.0.0-beta.2",
    "webpack": "^2.1.0-beta.7",
    "webpack-dev-server": "^2.1.0-beta.0"
  },
  "dependencies": {
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.0",
    "redux": "^3.5.2",
    "redux-saga": "^0.11.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": [
      "react-hot-loader/babel"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
      "prefer-template": 0
    }
  }
}
